{
  "name": "saccochain-backend",
  "version": "1.0.0",
  "description": "SACCOChain Backend API - Blockchain-powered SACCO management platform",
  "main": "src/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "build": "echo 'No build step required for Node.js'",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "node prisma/seed.js",
    "db:studio": "prisma studio",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "express-rate-limit": "^6.8.1",
    "@prisma/client": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "axios": "^1.4.0",
    "@mysten/sui.js": "^0.38.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.1.4",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "prisma": "^5.0.0",
    "nodemon": "^2.0.22",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8"
  },
  "keywords": [
    "sacco",
    "blockchain",
    "fintech",
    "credit-scoring",
    "kenya",
    "finance",
    "cooperative",
    "sui-blockchain"
  ],
  "author": "SACCOChain Team <hello@saccochain.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/saccochain/saccochain-backend"
  },
  "bugs": {
    "url": "https://github.com/saccochain/saccochain-backend/issues"
  },
  "homepage": "https://saccochain.com",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}