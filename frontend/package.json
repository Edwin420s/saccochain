  "repository": {
    "type": "git",
    "url": "https://github.com/saccochain/saccochain-frontend"
  },
  "bugs": {
    "url": "https://github.com/saccochain/saccochain-frontend/issues"
  },
  "homepage": "https://saccochain.com"
